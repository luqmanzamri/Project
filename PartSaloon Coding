double Dis2 = pay[2];
double studentprice1 = (Dis2+payfee[0])-total2;
System.out.println("");
System.out.println("**********RECEIPT**********");
System.out.println("");
System.out.println("Time and Date For Appointment :" + day +" " + " " + time +" "+" " +date);
System.out.println("");
System.out.println("Hair cut               : RM  "+ pay[2]);
System.out.println("Service fee            : RM  "+ payfee[0]);
System.out.println("Total dicount          : RM- "+ total2);
System.out.println("\n");
System.out.println("TOTAL price to be paid : RM  "+ studentprice1);
System.out.println("");
System.out.println("Thank you for your booking !");
System.out.println(" PS: cancel anytime"); }

