public static void printCalculate1(double[]pay,double[]payfee,String time,String date,String day) {       
double custprice = pay[2] + payfee[0];
System.out.println("");
System.out.println("**********RECEIPT**********");
System.out.println("");
System.out.println("Time and Date For Appointment :" + day +" " + " " + time +" "+" " +date);
System.out.println("");
System.out.println("Hair cut               : RM "+ pay[2]);
System.out.println("Service fee            : RM "+ payfee[0]);
System.out.println("\n");
System.out.println("TOTAL price to be paid : RM "+ custprice);
System.out.println("");
System.out.println("Thank you for your booking !");
System.out.println(" PS: cancel anytime");}
				      
